
profile single {
    output eDP-1
    exec ~/.config/kanshi/single.sh
}

profile dual {
    output eDP-1
    output *
    exec ~/.config/kanshi/dual.sh
}

profile work {
    output eDP-1
    output 'Dell Inc. DELL U2722DE HFMKDH3'  # DP-5
    output 'Dell Inc. DELL U2722D 3ZS8XN3'   # DP-6
    exec ~/.config/kanshi/work.sh
}
